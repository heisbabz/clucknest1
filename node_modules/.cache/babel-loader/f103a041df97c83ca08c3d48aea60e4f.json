A,UAAA,WAAA,EAAA,GADA;AAEA,UAAA,MAAA,EAAA,oBAFA;AAGA,UAAA,KAAA,EAAA;AAHA,SAAA,CAAA;AAKA,YAAA,GAAA,GAAA,EAAA,CAAA,UAAA,CAAA,OAAA,EAAA,GAAA,CAAA,KAAA,IAAA,CAAA;AACA,QAAA,GAAA,CAAA,GAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,MAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,2BAAA;AACA,WAFA,MAEA;AACA,YAAA,QAAA,CAAA,IAAA,GAAA,8BAAA,CAAA,KAAA,CAAA,KAAA,EAAA,KAAA,CAAA,QAAA,EACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,cAAA,GAAA,CAAA,GAAA,CAAA;AACA,gBAAA,OAAA,EAAA,IAAA,CAAA,GADA;AAEA,gBAAA,SAAA,EAAA,KAAA,CAAA,SAFA;AAGA,gBAAA,QAAA,EAAA,KAAA,CAAA,QAHA;AAIA,gBAAA,KAAA,EAAA,KAAA,CAAA,KAJA;AAKA,gBAAA,MAAA,EAAA,KAAA,CAAA,MALA;AAMA,gBAAA,QAAA,EAAA,KAAA,CAAA,QANA;AAOA,gBAAA,KAAA,EAAA,KAAA,CAAA,KAPA;AAQA,gBAAA,GAAA,EAAA,KAAA,CAAA,GARA;AASA,gBAAA,WAAA,EAAA,KAAA,CAAA,WATA;AAUA,gBAAA,IAAA,EAAA,KAAA,CAAA,IAVA;AAWA,gBAAA,KAAA,EAAA,KAAA,CAAA;AAXA,eAAA;AAaA,aAfA,EAeA,IAfA,CAeA,YAAA;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA;AAAA,eAAA;AACA,aAjBA,EAkBA,KAlBA,CAkBA,UAAA,GAAA,EAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,cAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,OAAA;AACA,aArBA;AAsBA;AACA,SA3BA;AA4BA,OAnCA,MAmCA;AACA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,EAAA,EADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA;AACA;AA5CA;AA3BA,CAAA","sourcesContent":["<template>\n    <div>\n        <Navbar />\n        <div class=\"register container shadow mt-5 p-4\">\n            <form class=\"mt-2 \" @submit.prevent=\"regSubmit\">\n                <h3 class=\"a\">REGISTER</h3>\n                <div class=\"form-row pt-3\">\n                    <div class=\"col\">\n                        <label for=\"firstName\">First Name</label>\n                        <input type=\"text\" class=\"form-control\" name=\"firstname\" placeholder=\"First name\" v-model=\"firstname\">\n                    </div>\n                    <div class=\"col\">\n                        <label for=\"lastName\">Last Name</label>\n                        <input type=\"text\" class=\"form-control\" name=\"lastname\" placeholder=\"Last name\" v-model=\"lastname\">\n                    </div>\n                </div>\n                <div class=\"form-row pt-3\">\n                    <div class=\"col-md-8\">\n                        <label for=\"email\">Email Address</label>\n                        <input type=\"email\" class=\"form-control\" name=\"email\" placeholder=\"Email\" v-model=\"email\">\n                    </div>\n                    <div class=\"col-md-4\">\n                    <label for=\"gender\">Gender</label>\n                    <select class=\"custom-select mr-sm-2\" name=\"gender\" v-model=\"gender\" placeholder=\"Email\">\n                        <option selected>Choose...</option>\n                        <option value=\"female\">Female</option>\n                        <option value=\"male\">Male</option>\n                    </select>\n                    </div>\n                </div>\n                <hr class=\"mt-3\">\n                <div class=\"form-row\">\n                    <div class=\"col-md-8\">\n                        <label for=\"farmName\">Farm Name</label>\n                        <input type=\"text\" class=\"form-control\" name=\"farmname\" placeholder=\"Farm name\" v-model=\"farmname\">\n                    </div>\n                    <div class=\"col-md-4\">\n                    <label for=\"dateOfEstablishment\">Date of Establishment</label>\n                    <datepicker input-class=\"form-control\" name=\"doe\" v-model=\"doe\" :disabledDates=\"disabledDates\" format=\"dd-MM-yyyy\"></datepicker>\n                    </div>\n                </div>\n                <div class=\"form-row pt-3\">\n                    <div class=\"col-md-8\">\n                        <label for=\"farmAddress\">Farm Address</label>\n                        <input type=\"text\" class=\"form-control\" name=\"farmaddress\" placeholder=\"Enter your farm address\" v-model=\"farmaddress\">\n                    </div>\n                    <div class=\"col-md-4\">\n                            <label for=\"farmName\">Phone Number</label>\n                            <input type=\"tel\" class=\"form-control\" name=\"phone\" v-model=\"phone\">\n                    </div>\n                </div>\n                <div class=\"form-row pt-3\">\n                    <div class=\"form-group col-md-6\">\n                        <label for=\"city\">City</label>\n                        <input type=\"text\" class=\"form-control\" name=\"city\" v-model=\"city\">\n                    </div>\n                    <div class=\"form-group col-md-4\">\n                        <label for=\"state\">State</label>\n                        <select class=\"form-control\" name=\"state\" v-model=\"state\">\n                            <option selected>Choose...</option>\n                            <option>Ekiti</option>\n                            <option>Kwara</option>\n                            <option>Lagos</option>\n                            <option>Ogun</option>\n                            <option>Ondo</option>\n                            <option>Osun</option>\n                            <option>Oyo</option>\n                        </select>\n                    </div>\n                </div>\n                <div class=\"form-row\">\n                    <div class=\"col-sm-7\">\n                        <label for=\"password\">Password</label>\n                        <input type=\"password\" class=\"form-control\" name=\"password\" placeholder=\"Enter your preferred password\" v-model=\"password\">\n                        <password-meter :password=\"password\" />\n                    </div>\n                </div>\n                <p v-if=\"feedback\" class=\"feedback\">{{ feedback }}</p>                \n                <button type=\"submit\" class=\"btn btn-primary mt-4 rb shadow\">Submit</button>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Navbar from '@/components/Navbar.vue'\nimport Datepicker from 'vuejs-datepicker'\nimport passwordMeter from \"vue-simple-password-meter\"\nimport slugify from 'slugify'\nimport db from '@/firebase/init'\nimport firebase from 'firebase'\n\nexport default {\n    name: 'Register',\n    components: {\n        Navbar,\n        Datepicker,\n        passwordMeter,\n    },\n    data(){\n        return{\n            slug: null,\n            firstname: null,\n            lastname: null,\n            email: null,\n            gender: null,\n            farmname: null,\n            phone: null,\n            doe: null,\n            farmaddress: null,\n            city: null,\n            state: null,\n            disabledDates: {\n                from: new Date(Date.now())\n            },\n            password: null,\n            feedback: null,\n        }\n    },\n    methods: {\n        regSubmit() {\n            if(this.farmname && this.farmaddress){\n              this.slug = slugify(this.farmname || this.farmaddress, {\n                  replacement: '-',\n                  remove: /[$*_+~.()'\"!\\-:@]/g,\n                  lower: true\n              })\n              let ref = db.collection('users').doc(this.slug)\n              ref.get().then(doc => {\n                  if(doc.exists){\n                      this.feedback = 'This input already exists'\n                  }else {\n                      firebase.auth().createUserWithEmailAndPassword(this.email, this.password)\n                      .then(cred => {\n                          ref.set({\n                            user_id: cred.uid,\n                            firstname: this.firstname,\n                            lastname: this.lastname,\n                            email: this.email,\n                            gender: this.gender,\n                            farmname: this.farmname,\n                            phone: this.phone,\n                            doe: this.doe,\n                            farmaddress: this.farmaddress,\n                            city: this.city,\n                            state: this.state,  \n                          })\n                      }).then(() => {\n                          this.$router.push({ name: 'Login'})\n                      })\n                      .catch(err => {\n                          console.log(err)\n                          this.feedback = err.message\n                      })\n                  }\n              }) \n            }else{\n                // this.feedback = 'You must enter all fields'\n                this.$toast.push({\n                    title:'',\n                    message:''\n                })\n            }\n        }\n    }\n}\n</script>>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.register {\n    color: #CD853F;\n    max-width: 700px;\n    background-color: #FFF;\n    border-radius: 10px;\n}\n.rb {\n    background-color: #FFF;\n    border: 1px solid #CD853F;\n    color: #CD853F;\n}\n.rb:hover {\n    background-color: #CD853F;\n    color: #FFF;\n}\n.feedback {\n    color: red;\n}\n</style>"],"sourceRoot":"src/views"}]}